{
	"manifest_version": 2,

	"name": "Seed Sync Push",
	"description": "Send a torrent to SeedSync",

	"version": "1.0",

	"permissions": [
		"http://*/"
	],

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["addbuttons.js"],
			"run_at": "document_idle",
			"all_frames": true
		}
	]
}